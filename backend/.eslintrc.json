{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "space-before-function-paren": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off"
  }
}
